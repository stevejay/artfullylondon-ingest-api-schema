{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://schemas.artfully.london/v1/feed-entity.json",
    "type": "object",
    "properties": {
        "entity": {
            "title": "Entity Type",
            "type": "string",
            "enum": [
                "event",
                "venue"
            ]
        },
        "venueId": {
            "title": "Venue ID",
            "allOf": [
                {
                    "$ref": "basic-types.json#/definitions/componentId"
                }
            ]
        },
        "venueEntityId": {
            "title": "Venue Entity ID",
            "allOf": [
                {
                    "$ref": "basic-types.json#/definitions/componentId"
                }
            ]
        },
        "version": {
            "title": "Entity Version",
            "allOf": [
                {
                    "$ref": "basic-types.json#/definitions/version"
                }
            ]
        },
        "url": {
            "title": "URL",
            "type": "string",
            "format": "uri"
        },
        "error": {
            "title": "Error Message",
            "type": "string",
            "minLength": 1
        },
        "stacktrace": {
            "title": "Error Stacktrace",
            "type": "string",
            "minLength": 1
        }
    },
    "required": [
        "entity",
        "venueId",
        "venueEntityId",
        "version"
    ]
}