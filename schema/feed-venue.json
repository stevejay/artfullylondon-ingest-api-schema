{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://schemas.artfully.london/v1/feed-venue.json",
    "type": "object",
    "properties": {
        "entity": {
            "title": "Entity Type",
            "const": "venue"
        },
        "venueId": {
            "title": "Venue ID",
            "allOf": [
                {
                    "$ref": "basic-types.json#/definitions/componentId"
                }
            ]
        },
        "venueEntityId": {
            "title": "Venue Entity ID",
            "allOf": [
                {
                    "$ref": "basic-types.json#/definitions/componentId"
                }
            ]
        },
        "version": {
            "title": "Venue Version",
            "allOf": [
                {
                    "$ref": "basic-types.json#/definitions/version"
                }
            ]
        },
        "rawContent": {
            "title": "Raw Content",
            "description": "The information to monitor for changes",
